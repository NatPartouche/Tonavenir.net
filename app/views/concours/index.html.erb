<h1>Listing concours</h1>

<table>
  <tr>
    <th>Titre</th>
    <th>Soustitre</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @concours.each do |concour| %>
  <tr>
    <td><%= concour.titre %></td>
    <td><%= concour.soustitre %></td>
    <td><%= link_to 'Show', concour %></td>
    <td><%= link_to 'Edit', edit_concour_path(concour) %></td>
    <td><%= link_to 'Destroy', concour, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

